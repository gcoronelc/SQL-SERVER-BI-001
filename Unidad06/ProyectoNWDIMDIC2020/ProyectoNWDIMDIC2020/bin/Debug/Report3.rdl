  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>NWDIMDIC2020</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ANIO">
            <Value>=Parameters!ANIO.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        PRODUCTO.nomProducto, PRODUCTO.nomCategoria, VENTAS_FACT.unidades, VENTAS_FACT.monto, TIEMPO.año, TIEMPO.mes
FROM            PRODUCTO INNER JOIN
                         VENTAS_FACT ON PRODUCTO.idProducto = VENTAS_FACT.idProducto INNER JOIN
                         TIEMPO ON VENTAS_FACT.fecha = TIEMPO.fecha
WHERE TIEMPO.año = @ANIO</CommandText>
      </Query>
